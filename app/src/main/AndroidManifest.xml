<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mb.assistant">


    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar" >
        <activity android:name=".CoverActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RoleActivity" android:screenOrientation="portrait"/>
        <activity android:name=".PropertyAndSkillActivity" android:screenOrientation="portrait"/>

    </application>
</manifest>
